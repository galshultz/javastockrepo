<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>StockPortfolioJavaCourse</servlet-name>
		<servlet-class>com.shultz.StockPortfolioJavaCourseServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StockPortfolioJavaCourse</servlet-name>
		<url-pattern>/stockportfoliojavacourse</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AdvancedMathExe</servlet-name>
		<servlet-class>com.shultz.AdvancedMathExe</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdvancedMathExe</servlet-name>
		<url-pattern>/advancedmathexe</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	

</web-app>
